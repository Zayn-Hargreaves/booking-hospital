{
    "version": 2,
    "builds": [
      {
        "src": "*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      },
      {
        "functions": {
          "api/**/*.js": {
            "maxDuration": 30 // Tăng thời gian chờ lên 30 giây
          }
        }
      }
    ]
  }
